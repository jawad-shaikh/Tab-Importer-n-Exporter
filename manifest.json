{
  "manifest_version": 3,
  "name": "Tab Exporter",
  "version": "1.0",
  "description": "Export tab group URLs",
  "permissions": ["tabs", "storage"],
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "images/icon16.png",
      "48": "images/icon48.png",
      "128": "images/icon128.png"
    }
  }
}
